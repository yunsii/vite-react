{
  "name": "vite-react",
  "version": "1.0.0",
  "author": {
    "name": "云深",
    "email": "yuns.xie@qq.com"
  },
  "scripts": {
    "dev": "cross-env BROWSER=chrome vite --host",
    "dev:force": "npm run dev -- --force",
    "build": "vite build",
    "serve": "vite preview --host",
    "start": "npm run dev",
    "lint": "npm run lint:js && tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "prettier": "prettier --check --write \"src/**/*\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">=12.2.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.16.1",
    "@vitjs/runtime": "^0.9.2",
    "ahooks": "^2.10.0",
    "antd": "^4.14.0",
    "classnames": "^2.2.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@types/babel__core": "^7.1.17",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@umijs/fabric": "^2.5.6",
    "@vitejs/plugin-react": "^1.1.1",
    "@vitjs/vit": "^0.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": ">=6",
    "less": "^4.1.1",
    "lint-staged": ">=10",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.2.4",
    "unplugin-auto-import": "^0.6.6",
    "vite": "^2.3.2",
    "vite-plugin-imp": "^2.0.4",
    "vite-plugin-windicss": "^1.8.3",
    "vite-tsconfig-paths": "^2.3.0",
    "windicss": "^3.5.1"
  }
}
